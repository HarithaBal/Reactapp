pipeline {
    agent any
        environment {
        registry = "519852036875.dkr.ecr.us-east-2.amazonaws.com/reactapp"
    }
     stages{
      stage ('K8S Deploy') {
		 steps{
                kubernetesDeploy(
                    configs: 'reactapp.yaml',
                    kubeconfigId: 'K8S',
                    enableConfigSubstitution: true
                    )           
            }
            
		      
        }
      
    }
     
   }

